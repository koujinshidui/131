        $(".zm-component-main").attr("contenteditable",false);
        $('.zm-dialog-box').remove();
        $('.zm-component-box1').each(function(){
            var _this = $(this);
            var type = zmEditor.component.attrType(_this);
            zmEditor.component[type].preview(_this);
        })
