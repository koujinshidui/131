@import "base.less";
/*组件列表中的样式*/
fieldset li .zm-component-carousel-main{
  overflow: hidden;
  width: 300px;
  height: 130px;
  position: relative;
  border: 1px dashed transparent;
}
fieldset li .zm-component-carousel-main:hover{
  border: 1px dashed @baseColor;
}
fieldset li .zm-component-carousel-main .zm-component-carousel-box img{
  width: 100%;height: 100%;
}
fieldset li .zm-component-carousel-main .zm-component-carousel-pageCutBtn{
  position: absolute;
  width: 100%;
  top: 46px;
  color: @baseColorOpacity9;
  font-size: 30px;
  left: 8px;
}
fieldset li .zm-component-carousel-main .zm-component-carousel-pageCutBtn .fa-angle-left{
  left: 8px;
  position: absolute;
}
fieldset li .zm-component-carousel-main .zm-component-carousel-pageCutBtn .fa-angle-right{
  right: 22px;
  position: absolute;
}
fieldset li .zm-component-carousel-main .zm-component-carousel-point{
  position: absolute;
  width: 100px;
  height: 10px;
  bottom: 5px;
  left: 100px;
}
fieldset li .zm-component-carousel-main .zm-component-carousel-point span{
  display: inline-block;
  width: 10px;
  height: 10px;
  background-color: @baseColorOpacity5;
  border-radius: 5px;
  margin: 0 0 5px 18px;
}
/*组件列表中的样式 end*/

/*页面中组件样式*/
.zm-component-carousel-main,.zm-component-carousel-slide,.zm-component-carousel-box,
.zm-component-carousel-box>div,.zm-component-carousel-slide img{
  width: 100%;
  height:100% ;
}
.zm-component-carousel-main .zm-component-carousel-box{
  overflow: hidden!important;
  box-shadow: 0 0 0 @baseFontColor;
  border: 1px solid transparent;
}
.zm-component-carousel-box .zm-component-carousel-slide{
  position: absolute;
}
.zm-component-carousel-box .thePieces:nth-child(1),
.zm-component-carousel-box .thePieces:nth-child(8),
.zm-component-carousel-box .thePieces:nth-child(33),
.zm-component-carousel-box .thePieces:nth-child(40){
  overflow: hidden;
}
.zm-component-carousel-picTip{
  position: absolute;
  top:-32px;
  right: 0;
  width: 150px;
  height: 30px;
  font-size: 14px;
  background-color: @baseColor;
  border-radius: 30px;
  text-align: center;
  color: @baseColorWhite;
  line-height: 30px;
  display: none;
}
.zm-component-carousel-prevPic,.zm-component-carousel-nextPic{
  font-size: 24px;
  border-radius: 24px;
  position: absolute;
  top:3px;
  background-color: @baseColor;
  cursor: pointer;
  /*z-index: 100;*/
  color: @baseColorLight1;
}
.zm-component-carousel-nextPic{
  right:3px;
}
.zm-component-carousel-prevPic{
  left: 3px;
  transform:rotate(180deg);
  -ms-transform:rotate(180deg); 	/* IE 9 */
  -moz-transform:rotate(180deg); 	/* Firefox */
  -webkit-transform:rotate(180deg); /* Safari 和 Chrome */
  -o-transform:rotate(180deg);
}
.zm-component-carousel-nextPic:hover,.zm-component-carousel-prevPic:hover {
  color: @baseColorWhite;
}
.maxWidth1200{
  max-width: 1200px;
  max-height: 500px;
}
/*页面中组件样式end*/

/*组件编辑样式*/
.zm-component-carousel-autoCut{
  width: 100%;
  height: 90px;
  left: 0;
  top: 38px;
  position: absolute;
  background:@baseColorOpacity9;
  z-index: 3;
}
.zm-edit-carousel-cutStyle{
  /*width: 265px;*/
  height: 95px;
  overflow: hidden;
  background:@baseColorLight1;
  position: relative;
  margin-top: 5px;
  border-radius: 3px;
}
.zm-edit-carousel-cutStyleList{
  /*margin-left: 30px;*/
}
.zm-edit-carousel-cutBtn{
  width:30px;
  height: 100%;
  position: absolute;
  background-color:@baseColorGray5 ;
  color: @baseColor;
  padding: 26px 4px;
  border-radius: 3px;
  z-index: 1;
}
.zm-edit-carousel-cutBtn:hover{
  background-color:@baseColor;
  color: @baseColorWhite;
  cursor: pointer;
}
.zm-edit-carousel-cutBtn.left{
  left: 0;
}
.zm-edit-carousel-cutBtn.right{
  right: 0;
  top: 0;
}
.zm-edit-carousel-cutStyleList li{
  float: left;
  width: 90px;
  height: 88px;
  position: relative;
  padding: 6px;
  font-size: 12px;
  text-align: center;
  margin: 4px 6px  8px;
  border: 1px solid transparent;
  box-sizing: border-box;
  border-radius: 3px;
}
.zm-edit-carousel-cutStyleList li:hover{
  border: 1px @baseColor solid;
  box-shadow: 0 0 3px @baseColor;
  background-color: @baseColorLight1;
  cursor: pointer;
}
.zm-edit-carousel-cutStyleList>li.styleCheck{
  border: 1px @baseColor solid;
  box-shadow: 0 0 3px @baseColor;
  background-color: @baseColorLight1;
  cursor: pointer;
}
.zm-edit-carousel-cutStyleList li img{
  width: 70px;
  height: 60px;
  opacity: .9;
  margin-bottom: 3px;
}
/*.zm-tab-child-title li.zm-tab-child-title-cur {*/
/*background-color: @baseColorWhite;*/
/*color: @baseColorDeep1;*/
/*}*/
.zm-component-carousel-controlBtn{
  position: absolute;
  color: @baseColorWhite;
  display: none;
  cursor: pointer;
  /*z-index: 2;*/
  /*opacity: .6;*/
}
.zm-component-carousel-controlBtn.left{
  left:10px;
}
.zm-component-carousel-controlBtn.right{
  right:10px;
}
.zm-edit-carousel-arrowList{
  height: 70px;
  overflow: hidden;
  background:@baseColorLight1;
  position: relative;
  margin-top: 5px;
  border-radius: 3px;
}
.zm-edit-carousel-arrowList li{
  float: left;
  width: 93px;
  height: 65px;
  color: @baseColorWhite;
  position: relative;
  font-size: 24px;
  text-align: center;
  margin: 5px 3px;
  padding: 7px 5px;
  border: 1px solid transparent;
  box-sizing: border-box;
}
.zm-edit-carousel-arrowList li div{
  float: left;
  width: 40px;
  height:50px;
  background-color: @baseColor;
  padding-top: 11px;
}
.zm-edit-carousel-arrowList li:hover{
  border: 1px @baseColor solid;
  box-shadow: 0 0 3px @baseColor;
  cursor: pointer;
}
.zm-edit-carousel-arrowList li.arrowCheck{
  border: 1px @baseColor solid;
  box-shadow: 0 0 3px @baseColor;
  cursor: pointer;
}

.zm-component-carousel-iSarrow{
  width: 100%;
  height: 92%;
  left: 0;
  top: 38px;
  position: absolute;
  background: @baseColorOpacity9;
  z-index: 3;
}
.zm-component-carousel-showPageNum{
  width: 100%;
  height: 466px;
  left:0;
  top: 41px;
  position: absolute;
  background: @baseColorOpacity9;
  z-index: 3;
}
.zm-component-carousel-pointGroup{
  position: absolute;
  bottom: 15px;
  /*z-index: 2;*/
  color: transparent;
  text-indent: 999px;
}
.zm-component-carousel-pagePoint{
  display: block;
  width: 10px;
  height: 10px;
  cursor: pointer;
  float: left;
  margin: 0 5px;
  background-color: @baseColorOpacity5;
  box-shadow: 0 0 20px @baseColorWhite;
}
.zm-component-carousel-pagePoints0{
  border-radius: 5px;
}
.zm-component-carousel-pagePoints1{
  border-radius: 0;
}
.zm-component-carousel-pagePoints2{
  border-radius: 0;
  width: 14px;
  height: 4px;
}
.zm-component-carousel-pagePoints3{
  border-radius: 0;
  text-align: center;
  width: 15px;
  height: 24px;
  color: @baseColorWhite;
  background-color: transparent;
  box-shadow: none;
  border: 1px solid transparent;
  text-indent: 0;
}
.zm-component-carousel-pagePoints3.itsTurn{
  background-color: transparent;
  border: 1px solid transparent;
}
.itsTurn{
  background-color: @baseColorWhite!important;
}

.zm-edit-carousel-pageNumList li{
  float: left;
  width: 82px;
  height: 58px;
  color: @baseColorWhite;
  position: relative;
  font-size: 24px;
  text-align: center;
  margin: 6px 10px;
  padding: 14px 5px;
  border: 1px solid transparent;
  box-sizing: border-box;
}
.zm-edit-carousel-pageNumList li:hover{
  border: 1px @baseColor solid;
  box-shadow: 0 0 3px @baseColor;
  cursor: pointer;
}
.zm-edit-carousel-pageNumList li.pointCheck{
  border: 1px @baseColor solid;
  box-shadow: 0 0 3px @baseColor;
  cursor: pointer;
}
.zm-edit-carousel-pageNumList li span{
  display: inline-block;
  width: 15px;
  height: 15px;
  border-radius: 50%;
  margin: 6px 4px;
  float: left;
  background-color: @baseColorGray1;
}
.zm-edit-carousel-pageNumList .pagePointStyle1 span{
  border-radius: 0;
}
.zm-edit-carousel-pageNumList .pagePointStyle2 span{
  border-radius: 0;
  width: 18px;
  height: 5px;
  margin: 11px 2px;
}
.zm-edit-carousel-pageNumList .pagePointStyle3 span{
  border-radius: 0;
  margin: 0 4px;
  color: @baseColorGray1;
  background-color: transparent;
}
.zm-edit-components-pageNumAlign{
  display: inline-block;
  text-align: center;
  width: 74%;
}
.zm-edit-components-pageNumAlign span{
  display: inline-block;
  margin: 0 10px;
  padding: 6px;
  color: @baseColorGray1;
  border: 1px solid transparent;
  border-radius: 3px;
}
.zm-edit-components-pageNumAlign span:hover{
  border: 1px solid transparent;
  box-shadow: 0 0 3px @baseColor;
  background-color: @baseColorLight1;
  cursor: pointer;
}
.zm-edit-components-pageNumAlign span.alignCheck{
  border: 1px solid transparent;
  box-shadow: 0 0 3px @baseColor;
  background-color: @baseColor;
  color: @baseColorWhite;
  cursor: pointer;
}
/*************图片编辑***************/
.zm-edit-carousel-imgManage{
  width: 100%;
  overflow: hidden;
  position: relative;
  height: 465px;
}
.zm-edit-carousel-imgManage .zm-edit-carousel-removeImg{
  color: @baseColorWhite;
  cursor: pointer;
  font-size: 16px;
  right: 3%;
  top: 2%;
  width: 26px;
  height: 26px;
  background-color: @baseColor;
  line-height: 26px;
  text-align: center;
  border-radius: 52%;
