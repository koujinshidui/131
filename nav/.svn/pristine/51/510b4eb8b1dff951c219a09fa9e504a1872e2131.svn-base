}
.setHrefBtn{
  background-color: @baseColor;
  box-shadow: 0 0 10px @baseColor;
}
.setHrefBtn:hover{
  background-color: @baseColorLight0;
  box-shadow: 0 0 10px @baseColorLight0;
}
.zm-edit-carousel-slideImgController{
  border-bottom: none;
  margin: 0;
  border-top: 1px solid @baseColorGray5;
  width: 100%;
  height: 50px;
  padding: 14px 115px;
}
.zm-edit-carousel-slideImgController button{
  width: 120px;
  height: 32px;
  border: none;
  outline:none;
  background-color: @baseColor;
  color: @baseColorWhite;
  font-size: 14px;
  border-radius: 16px;
  cursor: pointer;
}
.zm-edit-carousel-slideImgController button:hover{
  background-color: @baseColorLight0;
  box-shadow: 0 0 10px @baseColorLight0;
}
.zm-dialog-carousel-header{
  width: 100%;
  height: 40px;
  line-height: 40px;
  background-color: @baseColorLight1;
}
.zm-dialog-carousel-btns button {
  width: 100px;
  height: 26px;
  border: none;
  outline:none;
  background-color: @baseColor;
  color: @baseColorWhite;
  border-radius: 15px;
  margin: 0 5px;
  cursor: pointer;
}
.zm-dialog-carousel-btns button:hover{
  background-color: @baseColorLight0;
}
.zm-dialog-carousel-btns input[type=file]{
  width:100px;height: 26px;cursor: pointer;
  font-size: 0;text-indent: 1999px;
  border-radius: 15px;
  outline: none;border: none;
  position: absolute;
  right: 25px; top: 7px;
}
.zm-dialog-carousel-search{
  display: inline-block;
  float: right;
  position: relative
}
.zm-dialog-carousel-search span{
  position:absolute;
  font-size:16px;
  left:10px;
  top:12px;
  color: @baseColor;
}
.zm-dialog-carousel-search input{
  outline:none;
  width:200px;
  border-radius:15px;
  padding-left:30px;
  border: 1px solid #cbece9
}
.zm-dialog-carousel-search input:focus{
  border: 1px solid @baseColorLight0;
  box-shadow: 0 0 3px @baseColorLight0;
}

.zm-dialog-carousel-content .zm-dialog-carousel-contRightImgManage{
  width: 250px;
  height: 480px;
  float: right;
  padding: 25px 10px 0 10px;
  border-left: 2px solid @baseColorOpacity5;;
}
