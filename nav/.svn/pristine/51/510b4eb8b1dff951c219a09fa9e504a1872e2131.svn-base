}
.zm-edit-carousel-imgManage .zm-edit-carousel-cutPicBtn div:hover{
  background-color: @baseColorWhite;
}
.zm-edit-carousel-imgManage .zm-edit-carousel-box{
  /*height: 220px;*/
  overflow: hidden;
}
.zm-edit-carousel-imgManage .zm-edit-carousel-box img{
  width: 100%;
  height: 180px;
}
.zm-edit-carousel-imgManage .zm-edit-carousel-box .zm-edit-carousel-slide{
  float: left;
  /*height:220px;*/
  width: 100%;
}
.zm-edit-carousel-imgManage .zm-edit-carousel-box .zm-edit-carousel-slide .zm-edit-carousel-pages{
  position: absolute;
  width: 40px;
  text-align: center;
  right: 10px;
