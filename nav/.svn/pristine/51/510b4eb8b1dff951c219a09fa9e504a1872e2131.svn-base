  border: 1px solid @baseColorLight0;
  box-shadow: 0 0 3px @baseColorLight0;
}
.zm-edit-carousel-imgManage .zm-edit-carousel-imgCommitBtn{
  position: absolute;
  width: 100px;
  text-align: center;
  color: @baseColorWhite;
  left: 38%;
  top: 30%;
  font-size: 14px;
  height: 30px;
  line-height: 30px;
  background: @baseColor;
  border-radius: 15px;
  box-shadow: 0 0 15px @baseColor;
  cursor: pointer;
}
.zm-edit-carousel-imgManage .zm-edit-carousel-imgCommitBtn:hover{
  background: @baseColorLight0;
  box-shadow: 0 0 15px @baseColorLight0;
}
.zm-edit-carousel-imgManage .zm-edit-carousel-imgCommitBtn input[type=file]{
  width:100%;height: 100%;cursor: pointer;
  font-size: 0;text-indent: 1999px;
  position: absolute;left: 0; border-radius: 15px;
  outline: none;border: none;
}
.zm-edit-carousel-setHrefBtn{
  float: right;
  margin-right: 10px;
  width: 90px;
  border-radius: 15px;
