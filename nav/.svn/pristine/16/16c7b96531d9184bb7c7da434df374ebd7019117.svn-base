    /*background-color: #fff;*/
    /*color: #57b8ad;*/
/*}*/
.zm-component-carousel-controlBtn{
    position: absolute;
    color: #fff;
    display: none;
    cursor: pointer;
    /*z-index: 2;*/
    /*opacity: .6;*/
}
.zm-component-carousel-controlBtn.left{
    left:10px;
}
.zm-component-carousel-controlBtn.right{
    right:10px;
}
.zm-edit-carousel-arrowList{
    height: 70px;
    overflow: hidden;
    background:#DAF1EC;
    position: relative;
    margin-top: 5px;
    border-radius: 3px;
}
.zm-edit-carousel-arrowList li{
