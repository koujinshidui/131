}
.zm-edit-carousel-arrowList li.arrowCheck{
    border: 1px #70CBC1 solid;
    box-shadow: 0 0 3px #70cbc1;
    cursor: pointer;
}

.zm-component-carousel-iSarrow{
    width: 100%;
    height: 92%;
    left: 0;
    top: 38px;
    position: absolute;
    background: rgba(255,255,255,.9);
    z-index: 3;
}
.zm-component-carousel-showPageNum{
    width: 100%;
    height: 466px;
    left:0;
    top: 41px;
    position: absolute;
    background: rgba(255,255,255,.9);
    z-index: 3;
}
.zm-component-carousel-pointGroup{
    position: absolute;
    bottom: 15px;
    /*z-index: 2;*/
    color: transparent;
    text-indent: 999px;
}
.zm-component-carousel-pagePoint{
    display: block;
    width: 10px;
    height: 10px;
    cursor: pointer;
    float: left;
    margin: 0 5px;
    background-color: rgba(255,255,255,.4);
    box-shadow: 0 0 20px #fff;
}
.zm-component-carousel-pagePoints0{
    border-radius: 5px;
}
.zm-component-carousel-pagePoints1{
    border-radius: 0;
}
.zm-component-carousel-pagePoints2{
    border-radius: 0;
    width: 14px;
    height: 4px;
}
.zm-component-carousel-pagePoints3{
    border-radius: 0;
    text-align: center;
    width: 15px;
    height: 24px;
    color: #fff;
    background-color: transparent;
    box-shadow: none;
    border: 1px solid transparent;
    text-indent: 0;
}
.zm-component-carousel-pagePoints3.itsTurn{
    background-color: transparent;
    border: 1px solid transparent;
}
.itsTurn{
    background-color: rgba(255,255,255,1);
}

.zm-edit-carousel-pageNumList li{
    float: left;
    width: 82px;
    height: 58px;
    color: #fff;
    position: relative;
    font-size: 24px;
    text-align: center;
    margin: 6px 10px;
    padding: 14px 5px;
    border: 1px solid transparent;
    box-sizing: border-box;
}
.zm-edit-carousel-pageNumList li:hover{
    border: 1px #70CBC1 solid;
    box-shadow: 0 0 3px #70cbc1;
    cursor: pointer;
}
.zm-edit-carousel-pageNumList li.pointCheck{
    border: 1px #70CBC1 solid;
    box-shadow: 0 0 3px #70cbc1;
    cursor: pointer;
}
.zm-edit-carousel-pageNumList li span{
    display: inline-block;
    width: 15px;
