    padding: 6px;
    color: #aaaaaa;
    border: 1px solid transparent;
    border-radius: 3px;
}
.zm-edit-components-pageNumAlign span:hover{
    border: 1px solid transparent;
    box-shadow: 0 0 3px #70cbc1;
    background-color: #dbefed;
    cursor: pointer;
}
.zm-edit-components-pageNumAlign span.alignCheck{
    border: 1px solid transparent;
    box-shadow: 0 0 3px #70cbc1;
    background-color: #4AB1A7;
    color: #fff;
    cursor: pointer;
}
