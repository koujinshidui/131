    box-sizing: border-box;
}
.zm-edit-carousel-arrowList li div{
    float: left;
    width: 40px;
    height:50px;
    background-color: #57b8ad;
    padding-top: 11px;
}
.zm-edit-carousel-arrowList li:hover{
    border: 1px #70CBC1 solid;
    box-shadow: 0 0 3px #70cbc1;
