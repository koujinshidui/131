/*组件列表中的样式*/
 fieldset li .zm-component-carousel-main{
    overflow: hidden;
    width: 300px;
    height: 130px;
    position: relative;
    border: 1px dashed transparent;
}
 fieldset li .zm-component-carousel-main:hover{
    border: 1px dashed #4ab1a7;
}
 fieldset li .zm-component-carousel-main .zm-component-carousel-box img{
    width: 100%;height: 100%;
}
fieldset li .zm-component-carousel-main .zm-component-carousel-pageCutBtn{
    position: absolute;
    width: 100%;
    top: 46px;
    color: rgba(255,255,255,.8);
    font-size: 30px;
    left: 8px;
}
 fieldset li .zm-component-carousel-main .zm-component-carousel-pageCutBtn .fa-angle-left{
    left: 8px;
    position: absolute;
}
 fieldset li .zm-component-carousel-main .zm-component-carousel-pageCutBtn .fa-angle-right{
    right: 22px;
    position: absolute;
}
 fieldset li .zm-component-carousel-main .zm-component-carousel-point{
    position: absolute;
    width: 100px;
    height: 10px;
    bottom: 5px;
    left: 100px;
}
 fieldset li .zm-component-carousel-main .zm-component-carousel-point span{
    display: inline-block;
    width: 10px;
    height: 10px;
    background-color: rgba(255,255,255,.5);
    border-radius: 5px;
    margin: 0 0 5px 18px;
}
/*组件列表中的样式 end*/

/*页面中组件样式*/
.zm-component-carousel-main,.zm-component-carousel-slide,.zm-component-carousel-box,
.zm-component-carousel-box>div,.zm-component-carousel-slide img{
    width: 100%;
    height:100% ;
}
.zm-component-nowEdit .zm-component-carousel-main{
     box-sizing: border-box;
}
.zm-component-nowEdit .zm-component-carousel-main .zm-component-carousel-box{
    overflow: hidden;
    box-shadow: 0 0 0 rgba(0,0,0,1);
    border: 1px solid transparent;
}
.zm-component-carousel-picTip{
    position: absolute;
    top:-32px;
    right: 0;
    width: 150px;
    height: 30px;
    font-size: 14px;
    background-color: #4ab1a7;
    border-radius: 30px;
    text-align: center;
    color: #fff;
    line-height: 30px;
    display: none;
}
.zm-component-carousel-prevPic,.zm-component-carousel-nextPic{
    font-size: 24px;
    border-radius: 24px;
    position: absolute;
    top:3px;
    background-color: #4ab1a7;
    cursor: pointer;
    /*z-index: 100;*/
    color: #C9E8E5;
}
.zm-component-carousel-nextPic{
    right:3px;
}
.zm-component-carousel-prevPic{
    left: 3px;
    transform:rotate(180deg);
    -ms-transform:rotate(180deg); 	/* IE 9 */
    -moz-transform:rotate(180deg); 	/* Firefox */
    -webkit-transform:rotate(180deg); /* Safari 和 Chrome */
    -o-transform:rotate(180deg);
}
.zm-component-carousel-nextPic:hover,.zm-component-carousel-prevPic:hover {
   color: #fff;
}
.maxWidth1200{
    max-width: 1200px;
    max-height: 500px;
}
/*页面中组件样式end*/

/*组件编辑样式*/
.zm-component-carousel-autoCut{
    width: 96%;
    height: 90px;
    left: 0;
    top: 38px;
    position: absolute;
    background:rgba(255,255,255,.9);;
    z-index: 3;
}
.zm-edit-carousel-cutStyle{
    /*width: 265px;*/
    height: 95px;
    overflow: hidden;
    background:#DAF1EC;
    position: relative;
    margin-top: 5px;
    border-radius: 3px;
}
.zm-edit-carousel-cutStyleList{
    /*margin-left: 30px;*/
}
.zm-edit-carousel-cutBtn{
    width:30px;
    height: 100%;
    position: absolute;
    background-color:#EEEEEE ;
    color: #57b8ad;
    padding: 26px 4px;
    border-radius: 3px;
    z-index: 1;
}
.zm-edit-carousel-cutBtn:hover{
    background-color:#57b8ad ;
    color: #fff;
    cursor: pointer;
}
.zm-edit-carousel-cutBtn.left{
    left: 0;
}
.zm-edit-carousel-cutBtn.right{
    right: 0;
    top: 0;
}
.zm-edit-carousel-cutStyleList li{
    float: left;
    width: 90px;
    height: 88px;
    position: relative;
    padding: 6px;
    font-size: 12px;
    text-align: center;
    margin: 4px 6px  8px;
    border: 1px solid transparent;
    box-sizing: border-box;
    border-radius: 3px;
}
.zm-edit-carousel-cutStyleList li:hover{
    border: 1px #70CBC1 solid;
    box-shadow: 0 0 3px #70cbc1;
    background-color: #e0f3ef;
    cursor: pointer;
}
.zm-edit-carousel-cutStyleList>li.styleCheck{
    border: 1px #70CBC1 solid;
    box-shadow: 0 0 3px #70cbc1;
    background-color: #e0f3ef;
    cursor: pointer;
}
.zm-edit-carousel-cutStyleList li img{
    width: 70px;
    height: 60px;
    opacity: .9;
    margin-bottom: 3px;
}
