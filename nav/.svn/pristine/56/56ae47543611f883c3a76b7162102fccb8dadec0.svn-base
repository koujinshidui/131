/********************************/
//选中状态颜色
.choiceColor {
  color: rgb(255,255,255) !important;
  background-color: rgb(74,177,167) !important;
}
//划过状态颜色
.choiceColor:hover {
  background-color: #347c75!important;
}
//多选框
.checkbox {
  width: 16px;
  height: 16px;
  border-radius: 8px;
  border: 1px solid rgb(74,177,167);
  position: relative;
  cursor: pointer;
}
.checkboxAfter {
  content: "";
  width: 8px;
  height: 8px;
  border-radius: 4px;
  background-color: rgb(74,177,167);
  position: absolute;
  top: 3px;
  left: 3px;
}
/********************************/


.baseBorderRadiusMin{border-radius: 3px;};
.baseBorderRadiusSmall{border-radius: 6px;};
.baseBorderRadius{border-radius: 10px;};
.baseBorderRadiusBig{border-radius: 20px;};
.baseBorderRadiusMax{border-radius: 999px;};
.baseBorderRadiusPer50{border-radius: 50%;};

.baseBoxShadowMin{box-shadow: 0 0 1px #ccc;};
.baseBoxShadow{box-shadow: 0 0 3px #ccc;};
.baseBoxShadow2{box-shadow: 0 0 6px #ccc;};



