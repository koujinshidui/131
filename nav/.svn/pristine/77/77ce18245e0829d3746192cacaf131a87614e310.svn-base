            componentsUl.stop().slideToggle();
            var componentsTitle = _this.find('.zm-components-type-switch-title');
            if(componentsUl.css('display')=="none"){
                componentsTitle.hide();
            }
            else{
                componentsTitle.show();
            }
            $(".zm-components-detail").stop().fadeOut()
        })
