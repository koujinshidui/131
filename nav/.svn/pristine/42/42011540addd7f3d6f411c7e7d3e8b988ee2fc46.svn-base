/*站点设置*/
.zm-website-set{position:relative;cursor:pointer;padding: 0 20px;}
.zm-website-set:hover{background-color:#ddd}
.zm-website-set:hover ul{display:block;}
.zm-website-set>ul{position:absolute;left:0;top:100%;padding:10px;overflow: hidden; width:500%;box-shadow:0 0 10px #ccc;display:none;background-color:@baseColorWhite;color:#444;z-index: 91;border-radius: 10px;border: 1px solid #eee;line-height: 35px;}
.zm-website-set>ul>li{position:relative;vertical-align: top;float: left;width:25%;line-height: 28px;text-align: center;border-radius: 8px;padding:6px;}
.zm-website-set>ul>li>span{font-size: 20px;}
.zm-website-set>ul>li:hover{background-color:@baseColorLight1;}
/*头部左侧*/
.zm-header-left{position:absolute;top:0;left:0;}
.zm-header-left>div{border-right:1px solid #eee;height:100%}
/*页面设置*/
.zm-page-set{position:relative;cursor:pointer;padding:15px 20px 0 20px;background: @baseColorLight1}
.zm-page-set:hover{background:@baseColor;color:@baseColorWhite}
.zm-option-group1{position:relative;padding:0;font-size: 12px;}
.zm-option-group1 img{height:20px;}
.zm-option-group1>div{float: left;text-align: center;padding:3px;margin:6px 3px 0 3px;cursor:pointer;border-radius:4px;}
.zm-option-group1>div:hover{background-color:@baseColorLight1;}
.zm-option-group2{font-size: 12px;}
.zm-option-group2>div{height:25px;line-height:25px;cursor: pointer;padding: 0 10px}
.zm-option-group2>div:hover{background-color:@baseColorLight1;}
.zm-option-group3{position:relative;padding:0;font-size: 14px;}
.zm-option-group3 img{height:20px;}
.zm-option-group3>div{float: left;text-align: center;padding:6px 10px 0 10px;cursor:pointer;height:50px;}
.zm-option-group3>div:hover{background-color:@baseColorLight1;}
.zm-logo{cursor:pointer;padding: 0px 20px 0 20px;color:deepskyblue;line-height: 50px}
.zm-logo>img{}
.zm-logo-pointer{display: inline-block;border-radius:50%;width:5px;height:5px;background-color:deepskyblue;margin:0 6px;top:-3px;position:relative}

/*组件列表部分*/
.zm-components{position:relative;font-size: 12px;z-index: 998;user-select: none;}
.zm-components-type{position:fixed;top:80px;left:20px;color:@baseColorWhite;}
.zm-components-type-switch{position:relative;border-bottom:1px solid @baseColorWhite;width:0;padding:0 6px;color:@baseColorWhite;height:48px;line-height:48px;font-size: 14px;font-weight: bold;}
.zm-components-type-switch-icon{position: absolute;left:0;top:0;z-index: 2;width:48px;height:48px;border-radius: 50%;background-color: @baseColor;text-align: center;font-size: 30px;cursor:pointer;}
.zm-components-type-switch-title{position: absolute;left:0;top:0;z-index: 1;width:130px;height:48px;border-radius: 50px;background-color: @baseColor;padding-left:50px;cursor:pointer;}
.zm-components-type-switch-icon span{position:relative;animation:componentsSwitchIcon infinite 1s}
@keyframes componentsSwitchIcon { 0%{top:-3px;} 50%{top:3px;} 100%{top:-3px;} }
.zm-components-ulBox{display:none;box-shadow: 0 0 10px #aaa;border-radius:10px ;position:absolute;left:0;top:0;background-color:@baseColor;width:130px;}
.zm-components-ul-title{height:50px;line-height: 50px;background-color: @baseColor;padding:0 8px;border-radius:10px 10px 0 0 ;border-bottom:1px solid @baseColorWhite;}
.zm-components-ul{border-radius:0 0 10px 10px;}
.zm-components-ul li{float: left;width: 54px;  height: 54px;  margin: 5px;text-align: center;padding:8px 0;cursor:pointer;border-radius: 50%;}
/*.zm-components-ul>li:nth-child(2n){border-left:1px solid #eee;}*/
.zm-components-ul li>img{height:24px;margin:5px 0}
.zm-components-ul li .fa{font-size: 20px;}
@keyframes hoverLi{100%{background-color:@baseColorWhite;color:@baseColor}}
.zm-components-ul li:hover{background-color:@baseColorDeep2;color:@baseColorWhite}
//.zm-components-ul li.zm-components-type-curLi{background-color:@baseColorDeep2;color:@baseColorWhite}
.zm-components-ul li.zm-components-type-curLi{background-color:@baseColorWhite;color:@baseColor;}
.zm-components-ul li.zm-components-type-curLi svg{fill:@baseColor}
.zm-components-li-svgBox{width:22px;height:22px;margin:0 auto;}
.zm-components-li-svgBox svg{fill:#fff;width:100%;height:100%;}
/*组件部分详情窗口*/
.zm-components-detail{display: none; width:350px;background-color:@baseColorWhite;border-radius: 10px;position:fixed;top:80px;left:155px;box-shadow: 0 0 10px #ccc;}
.zm-components-detail-header{position:relative;height:50px;line-height: 50px;border-bottom:1px solid #aaa;background-color: @baseColor;color:@baseColorWhite;border-radius: 10px 10px 0 0 ;padding: 0 20px}
.zm-components-detail-title{padding-left:6px;font-size: 16px;font-weight: bold}
.zm-box-header-btn{cursor:pointer;margin: 12px 0 0 8px;float:right;color:@baseColor;outline: 0;background: @baseColorLight1; text-align: center;width:26px;height:26px;line-height:26px;border-radius: 50%;}
.zm-box-header-btn:hover{ background: @baseColorWhite}
/*详情标题锚点*/
