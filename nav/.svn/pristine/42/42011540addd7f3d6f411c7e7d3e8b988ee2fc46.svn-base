.zm-tooltipBox{position:fixed;border-radius:8px;text-align:left;background-color:@baseColorLight1;box-shadow:0 0 5px @baseColorGray1;color:#333;padding:8px 15px;z-index: 9999;font-size: 12px;}
.zm-tooltipBox:after, .zm-tooltipBox:before {  border: solid transparent;content: ' '; height: 0; top: 100%;left: 50%;margin-left:-10px ;position: absolute;width: 0;  }
.zm-tooltipBox:after {border-width: 10px;border-top-color: @baseColorLight1;  }
.zm-tooltipBox:before {border-width: 10px;border-top-color: @baseColorLight1;  }



/*页面设置窗*/
.zm-page-setting-ul{}
.zm-page-setting-ul li{border-radius: 5px;position:relative;margin:5px 20px;width:300px;padding:6px 10px}
/*border:1px solid deepskyblue;height:36px;line-height: 36px;*/
.zm-page-setting-right-icon{position:absolute;right:10px;  top: 10px;cursor:pointer}
.zm-page-setting-right-icon:hover{color:deepskyblue}
.zm-page-setting-a{margin-left:5px;}
.zm-page-setting-top-span{ position: absolute;  top: -21px;  left: 50%;  color: @baseColor;  font-size: 30px;  margin-left: -15px;}
.zm-page-setting-add{margin:20px 0;text-align: center;}
.zm-page-setting-add a{height:32px;line-height: 32px;background-color:@baseColor;border-radius: 20px;color:@baseColorWhite;width:220px;display: inline-block;cursor:pointer;letter-spacing:0.1em}
.zm-page-setting-add a:hover{background-color:#207d74;}
.zm-page-setting-menu{position:fixed;z-index:999;background-color:@baseColorWhite;border: 1px solid #eee;border-radius: 10px;padding:10px 20px;box-shadow: 0 0 8px #ccc;}
.zm-page-setting-menu p{height:30px;line-height: 30px;cursor:pointer;color:#000}
.zm-page-setting-menu p:hover{color:deepskyblue;}
.zm-page-setting-menu p a{margin-left:5px;}
.zm-page-setting-menu-point:before{ content: "";  position: absolute;  left:-24px;  top:10px;  width: 0px;  height: 0px;   border: 12px solid transparent;  border-right-color:#ddd;  }
.zm-page-setting-menu-point:after{ content: "";  position: absolute;  left:-23px;  top:10px;   width: 0px;  height: 0px;   border: 12px solid transparent;  border-right-color:@baseColorWhite;  }
.zm-dialog-content-a{padding:0 20px;width: 600px;height: 406px;box-shadow: 0 0 1px 1px  #cccccc;border-bottom-left-radius: 5px;border-bottom-right-radius:5px }
.zm-page-setting-textarea{height:60px;width:300px;resize:none;}
.zm-page-setting-btns{width:600px;height:50px;text-align: center;line-height: 50px;border-top: 1px solid @baseColorLight1;margin-left: -20px}
.zm-page-setting-btns a{border:none;border-radius: 20px;height:30px;line-height: 30px;text-align: center;width:100px;display:inline-block;color:@baseColorWhite;margin:0 10px;cursor:pointer}
.zm-page-setting-btns a:nth-child(1){background-color:#aaaaaa;float: left;margin-left: 40px;margin-top: 10px;}
.zm-page-setting-btns a:nth-child(1):hover{background-color:#cccccc}
.zm-page-setting-btns a:nth-child(2){background-color:#49b1a6;float: right;margin-right: 40px;margin-top: 10px;}
.zm-page-setting-btns a:nth-child(2):hover{background-color:#00c3b0;}


