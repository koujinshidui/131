.zm-components-sort-save,.zm-components-sort-cancel{display: none;}
.zm-components-detail-content{position:relative;  }
.zm-components-detail-content>div{display:none;}
fieldset.zm-component-fieldset{ font-size:14px; padding:15px;}
fieldset.zm-component-fieldset legend{width:100%;text-align: left;padding: 10px 0;font-size:18px; border-bottom:1px solid #ccc;font-weight: bold;}
fieldset.zm-component-fieldset>ul>li{border:1px solid transparent;float: left;cursor:move;padding:5px;margin:5px;line-height: 1;position:relative;}
fieldset.zm-component-fieldset>ul>li.zm-components-detail-contentSortLi{border:1px dashed @baseColor;;}
.zm-components-detail-term-text li{float: none}
fieldset.zm-component-fieldset>ul>li:hover{border:1px solid @baseColor}
/*组件样式选择窗样式*/
fieldset.zm-component-fieldset>ul>li .zm-component-settingPanel-curLiFlag{position:absolute;right:0;top:0;line-height:18px;text-align:center;color:#fff;border:2px solid #fff;height:22px;width:22px;background: @baseColor;border-radius:50%;}

/*主体内容*/
.zm-container{position:relative}
.zm-all{position:relative;color:#000;box-sizing: border-box;overflow-x: hidden;overflow-y: scroll;}
.zm-page{display:none;}
.zm-page-cur{display:block;}
.zm-row-full{position: relative;}
.zm-row{position:relative;margin:0 auto;height:700px;border-left:1px solid @baseColor;border-right:1px solid @baseColor}
.zm-head .zm-row{height:150px;}
.zm-foot .zm-row{height:150px;}
.zm-row:hover .zm-row-resize{display: block}
.zm-row-resize{position:absolute;right:50px;bottom:10px;cursor:s-resize;display:none;z-index: 2;}
.zm-row-resize>span{position:absolute;display: inline-block;width:20px;height:20px;line-height:20px;text-align:center;font-weight:bolder;font-size:14px;background-color:@baseColorWhite;border-radius: 5px;color:#000;box-shadow: 0 0 8px #999;}
.zm-row-type{display: none;position:absolute;left:0;top:0;}
.zm-row-type>span{position:absolute;display: inline-block;height:20px;line-height:20px;width:150px;color:@baseColorWhite;text-align:center;background-color:#ec971f}
.zm-row:hover .zm-row-type{display: block}
.zm-row-line{width:100%;height:5px;background:linear-gradient(to bottom,@baseColorLight1 0,@baseColor 100%)}

/*组件box*/
.zm-component-editable{z-index: 999;}
.zm-editor-selected{background-color:#ccc;}
.zm-component-movable{cursor:move}
.zm-component-editor{}
/*.zm-component-editor:hover .zm-component-edit{display: block}*/
.zm-component-box1{position:absolute;border: 1px dashed transparent;}
.zm-component-nowEdit{}
.zm-component-main{border: 1px solid transparent}
.zm-component-nowEdit>.zm-component-box2>.zm-component-main{border: 1px solid @baseColor}
.zm-component-box2{position:relative;width:100%;height:100%;}
.zm-component-main-temp{position:absolute;width:100%;height:100%;border: 1px solid transparent;}
.zm-component-main-tempBorder{border: 1px solid @baseColor}
.zm-component-main{font-size: 14px;outline: 0;line-height: 1.6;position:relative;}
.zm-component-edit{position:absolute;top:-64px;left:0;display: none;padding-bottom:5px;z-index: 2;min-width: 140px;}
.zm-component-edit.topper{top:-86px;}
.zm-component-edit>button{height:36px;line-height:32px;width:36px;box-shadow:0 2px 6px 0 #ccc;border:none;text-align:center;cursor:pointer;
  background: linear-gradient(to bottom, white 0%, #eeeeee 100%);color: @baseColor;outline: 0;border-radius: 50%;    margin-right: 10px;font-size: 22px;}
.zm-component-edit>button:hover{background:@baseColor;color:@baseColorWhite;border:2px solid @baseColorWhite;line-height: 28px;}
/*.zm-component-edit>button:active{background-color:#298770;}*/
.zm-component-location{position:absolute;left:0;top:-24px;font-size: 12px;color:@baseColorWhite;background:@baseColor;display: none;
  border-radius:10px;text-align: center;padding:1px 8px;z-index: 2;white-space:nowrap;cursor:default;}
.zm-component-location.topper{top:-46px}
.zm-component-resize{display: none}
.zm-component-resize>span{position:absolute;z-index:2;display: inline-block;width:5px;height:5px;border-radius: 50%;border:1px solid @baseColor;background-color: @baseColorWhite;}
.zm-component-resize>span:nth-child(1){left:-3px;top:-3px;cursor:nw-resize}
.zm-component-resize>span:nth-child(2){left:50%;top:-3px;margin-left:-3px;cursor:n-resize}
.zm-component-resize>span:nth-child(3){right:-3px;top:-3px;cursor:ne-resize}
.zm-component-resize>span:nth-child(4){right:-3px;top:50%;margin-top:-3px;cursor:e-resize}
.zm-component-resize>span:nth-child(5){right:-3px;bottom:-3px;cursor:se-resize}
.zm-component-resize>span:nth-child(6){left:50%;bottom:-3px;margin-left:-3px;cursor:s-resize}
.zm-component-resize>span:nth-child(7){left:-3px;bottom:-3px;cursor:sw-resize}
.zm-component-resize>span:nth-child(8){left:-3px;top:50%;margin-top:-3px;cursor:w-resize}

.zm-component-rotate{position:absolute;left:-26px;top:-20px;color:@baseColor;font-size: 14px;
height:22px;line-height:22px;width:22px;box-shadow:0 2px 6px 0 #ccc;border-radius:50px;text-align: center;
  background:linear-gradient(to bottom, white 0%, #eeeeee 100%);}
.zm-component-rotate:hover{background:@baseColor;color:@baseColorWhite;border:2px solid @baseColorWhite;line-height: 18px;}

/*组件发布保存为草稿按钮专用遮罩层*/
.zm-publish-save-mask{position:absolute;right:115px;top:0;width:115px;height:50px;background: rgba(0,0,0,0.3);z-index: 2}

/*所有模板*/
.zm-w1400{width:1400px;margin:0 auto;}
.zm-template-all{position:fixed;left:0;top:-100%;width:100%;height:100%;background-color:@baseColorWhite;color:#000;z-index: 9999;overflow: hidden;}
.zm-template-list{position:absolute;left:0;top:0;width:100%}
.zm-template-all-header{height:70px;line-height: 70px;border-bottom:1px solid #ccc;width:100%;}
.zm-template-all-header-left{float:left;font-size: 20px;font-weight: bold}
.zm-template-all-header-right{float: right}
.zm-template-all-close{font-size: 20px;cursor:pointer;}
.zm-template-all-close:hover{color:red}
.zm-template-all-body{padding:20px 0}
.zm-template-all-body-left{float: left;}
.zm-template-all-body-left-ul{border-radius: 10px;background-color:#4c4c4c;width:170px;}
.zm-template-all-body-left-ul>li{height:60px;line-height:60px;border-bottom: 1px solid #424242;text-align: center;color:#afafaf;font-size:16px;position:relative;}
.zm-template-all-body-left-ul>li>span:nth-child(2){margin-left:10px;}
.zm-template-all-body-left-ul>li:hover{background-color:deepskyblue;color:@baseColorWhite;}
.zm-template-all-menu{z-index:999;position:absolute;top:0;left:100%;width:900px;text-align: left;padding-left:50px;display: none;background-color:#f5f5f5;border:1px solid #ccc}
.zm-template-all-menu>div{float: left;width:30%;font-size: 12px;color:#999;line-height: 24px;}
.zm-template-all-menu>div>p{height:40px;line-height: 40px;font-size: 14px;color:#666;}
.zm-template-all-menu>div a{margin-right:10px;cursor:pointer}
.zm-template-all-menu>div a:hover{color:deepskyblue}
.zm-template-all-body-right{float: right;border:1px solid #eee;;width:1200px;overflow: auto;}
.zm-template-item{position:relative;float:left;margin: 20px 36px;width:320px;height:215px;    text-align: center; overflow: hidden;border:1px solid #eee;}
.zm-template-item img{width:100%}
.zm-template-item:hover .zm-template-item-hover{display: block}
.zm-template-item-hover{position:absolute;left:0;top:0;width:100%;height:100%;background-color:rgba(0,0,0,0.4);display: none;}
.zm-template-item-hover>div{position:absolute;left:0;top:50%;height:40px;width:100%;line-height: 40px;margin-top:-20px;}
.zm-template-item-hover>div>a{border-radius: 30px;cursor:pointer;background-color:deepskyblue;color:@baseColorWhite;display:inline-block;width:110px;height:40px;margin:0 6px;font-size: 16px}
.zm-template-item-hover>div>a:hover{background-color:#0d487f;}

.zm-template-view{display:none;position:absolute;left:0;top:0;width:100%}
.zm-template-view-o{float: right;}
.zm-template-o-a{display: inline-block;height:35px;line-height: 35px;width:120px;background-color:deepskyblue;border-radius: 50px;text-align: center;color: @baseColorWhite;margin: 0 5px;cursor:pointer;}
.zm-template-o-a:hover{background-color:#0d487f;}
.zm-template-view-body{overflow: auto;}

/*switch*/

.zm-switch-box{display:inline-block;width:50px;border-radius: 10px;height:20px;cursor:pointer;background-color:@baseColorLight1;text-align: left;}
.zm-switch-box-on{background-color:@baseColor;text-align: right}
.zm-switch{border-radius: 10px;height:20px;line-height: 18px;background-color: @baseColorWhite;width:20px;display: inline-block; text-align: center;color: @baseColor;border:1px solid @baseColor;}

/*checkbox*/
.zm-checkbox{position:relative;top:2px;display: inline-block;width:16px;line-height:16px;height:16px;border:1px solid #ccc;border-radius: 16px;font-size: 12px;}
.zm-checkbox-icon{color:#4AB1A7;position:absolute;left:1px;top:2px;display: none;}
.zm-checkbox-name{display: inline-block;margin:0 5px;}
.zm-checkbox-prompt{color:#ccc;font-size: 12px;}
.zm-choiceHref-choice:after {
  content: "";
  width: 10px;
  height: 10px;
  border-radius: 5px;
  background-color: #4ab1a7;
  position: absolute;
  top: 0px;
  left: 1px;
}
/*选项卡1样式---通用*/
.zm-tab{font-size:14px;}
.zm-tab-title{margin:0;background-color:@baseColorWhite;height:34px;line-height: 31px;width:100%;border-bottom:3px solid #eee;color:@baseColor;font-size: 16px}
.zm-tab-title li{float: left;cursor:pointer;position:relative;text-align: center}
.zm-tab-title li:hover{border-bottom:3px solid @baseColor}
.zm-tab-title li.zm-tab-title-cur{border-bottom:3px solid @baseColor}
.zm-tab-title li.zm-tab-title-cur:before{  content: "";  position: absolute;  bottom: 0;  left: 50%;  width: 0px;  height: 0px;  margin-left:-3px;  border: 5px solid transparent;  border-bottom-color:@baseColor;  }
.zm-tab-content{width:350px;overflow: hidden}
.zm-tab-content>div{display:none;position:relative;}
.zm-tab-content>div.zm-tab-content-cur{display: block}

/*选项卡2样式---通用*/
.zm-tab-other{font-size:14px;}
.zm-tab-other-title{margin:0;background-color:@baseColorWhite;height:60px;line-height: 57px;width:100%;border-bottom:3px solid #eee;color:@baseFontColor;font-size: 16px}
.zm-tab-other-title li{float: left;cursor:pointer;position:relative;text-align: center;padding:0 10px}
//.zm-tab-other-title li:hover{border-bottom:3px solid @baseColor}
//.zm-tab-other-title li.zm-tab-other-title-cur{border-bottom:3px solid @baseColor}
//.zm-tab-other-title li.zm-tab-other-title-cur:before{  content: "";  position: absolute;  bottom: 0;  left: 50%;  width: 0px;  height: 0px;  margin-left:-3px;  border: 5px solid transparent;  border-bottom-color:@baseColor;  }
.zm-tab-other-content{width:350px;overflow: hidden}
.zm-tab-other-content>div{display:none;position:relative;}
.zm-tab-other-content>div.zm-tab-other-content-cur{display: block}

/*选项卡3样式---通用*/
.zm-tab-child{font-size:14px;}
.zm-tab-child-title{float:left;width:45px;margin:0;background-color:@baseColorLight1;height:557px;border-radius: 0 0 0 10px;color:@baseColor}
.zm-tab-child-title li{cursor:pointer;position:relative;text-align: center;padding:10px 0;}
.zm-tab-child-title li:hover{background:@baseColorLight2;}
.zm-tab-child-title li.zm-tab-child-title-cur{background-color: @baseColorWhite;color:#333}
.zm-tab-child-content{float: left;width:305px;;overflow: hidden}
.zm-tab-child-content div.zm-tab-child-content-sub{display:none;position:relative;}
.zm-tab-child-content div.zm-tab-child-content-cur{display: block}

/*拖动排序盒子*/
.zm-drag-box li{float: left;list-style: none;}
.zm-drag-box-tempLi{border:1px dashed @baseColor; background: @baseColorLight2; color: @baseColorWhite;text-align: center;padding:0;}

