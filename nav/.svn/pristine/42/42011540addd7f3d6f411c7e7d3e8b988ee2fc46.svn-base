@import "base.less";

html{height:100%;}
body{height:100%;min-width: 1400px;overflow: hidden;font-size: 14px;font-family: "微软雅黑";color:#000;}
input[type='text']{border:1px solid #ccc;height:30px;padding:0 10px;border-radius: 4px;}
select{border-radius: 5px;border:1px solid #ccc;height:30px;padding:0 10px;}
input,textarea,button{font-family: "微软雅黑"}
.zm-color001{background:@baseColorWhite;}
.zm-color002{background:#3899ec;}
.zm-color003{background:#428bca;}
.zm-color004{background:#5cb85c;}
.zm-color005{background:#f0ad4e;}
.zm-color006{background:#d9534f;}
.zm-color007{background:red;}
.zm-color008{background:#000;}
.zm-color009{background:#ccc;}
.zm-ul-1li li{width:100%;box-sizing: border-box}
.zm-ul-3li li{width:33.3%;box-sizing: border-box}
.zm-span-120{display: inline-block;min-width:60px;margin-right:8px;font-weight: bold}
.zm-input-250{width:300px;}
.zm-title{font-weight: bold;}
.zm-span-left{float: left}
.zm-margin-left-100{margin-left:70px;}
.zm-w1200{width:1200px;margin:0 auto;}

//拖拽辅助线
#zm-xHelpLine{display:none;position:fixed;width:100%;background:#e00eeb;height:1px;left:0;top:0;z-index: 9999}
#zm-yHelpLine{display:none;position:fixed;width:1px;background:#e00eeb;height:100%;left:0;top:65px;z-index: 9999}
/*鼠标右击样式*/
#zmRightClickAll{position: absolute;width:220px;display:none;z-index: 999}
.zmMouseRightClickUl1{width:100%;font-family:"微软雅黑";background:rgba(255,255,255,0.9);border:1px solid #ccc;border-radius: 5px;padding:5px 0;margin:0;cursor:pointer;}
.zmMouseRightClickUl1 li{position:relative;list-style: none;height:30px;line-height: 30px;color:#000;font-size: 12px;padding:0 15px;;margin:0;}
.zmMouseRightClickUl1 li:hover{background-color:#4ab1a7;color:@baseColorWhite;}
.zmMouseRightClickUl1>li>span{float: right;margin-right:10px;}
.zmMouseRightClickUl1>li>span.fa{margin-top:10px;}
.zmMouseRightClickUl1>li>ul{position:absolute;top:-5px;display:none;width:100px;font-family:"微软雅黑";background-color:@baseColorWhite;border:1px solid #ccc;border-radius: 5px;padding:5px 0;margin:0;cursor:pointer;}
.zmMouseRightClickUl1>li:hover ul{display: block;}
/*提示信息*/
.zm-prompt-box{background-color:rgba(0,0,0,0.2);position:fixed;height:100%;width:100%;z-index: 9999;text-align: center;display: none}
.zm-prompt{margin:100px auto;background-color:@baseColorWhite;width:550px;height:160px;line-height:36px;overflow:hidden;border-radius: 50px;box-shadow:0 0 10px #aaa;font-size: 20px;color:#aaa;  position: relative;  }
.zm-prompt>div{float: left;width:70%;height:100%;}
.zm-prompt>div:nth-child(1){width:70%;padding:10px 0;border-right:1px solid #ccc;box-sizing: border-box}
.zm-prompt>div:nth-child(2){width:30%}
.zm-prompt span{box-shadow:0 0 10px #aaa;display:inline-block;background-color:deepskyblue;color:@baseColorWhite;cursor:pointer;border-radius: 50%;font-size:30px;width: 80px;height:80px;line-height:80px;position:relative;top:35px;}
.zm-prompt span:hover{background-color:dodgerblue}

/*.zm-editor-beforeBox{display: none}*/
.zm-editor-beforeBg{position:fixed;width:100%;height:100%;z-index: 9998;background-color:#000;opacity: 0.3}
.zm-editor-type{position:fixed;width:600px;height:350px;top:50%;left:50%;margin-left:-300px;margin-top:-180px;
  border-radius: 10px;box-shadow: 0 0 20px #666;
  background-color:@baseColorWhite;z-index: 9999;}
.zm-editor-type li{float: left;width:150px;height:150px;margin:95px 0 0 38px;border:1px solid @baseColor;color:@baseColor;border-radius: 10px;text-align: center;cursor:pointer}
.zm-editor-type li:hover{box-shadow: 0 0 10px @baseColor}
.zm-editor-typeIcon{font-size: 100px;}
.zm-editor-typeName{font-size: 20px;}

/*头部刻度尺*/
.zm-header-bottom{position:relative;height:17px;font-size: 12px;color:#333;}
.zm-editor-ruler-x-switch-box{position:absolute;cursor:w-resize;height:15px;width:10px;margin-left:-5px;padding-left:5px;z-index: 90}
.zm-editor-ruler-x-switch{position:relative;display:inline-block;height:40px;width:1px;background-color:@baseColor;}
.zm-editor-ruler-x-width{position:absolute;left:0px;top:15px;font-size:10px;color:@baseColor}
.zm-editor-ruler-x-switch-span{position:absolute;left:-6px;top:0;width:0;  height:0;  border-width:6px 6px 0;  border-style:solid;  border-color:@baseColor transparent transparent;  }
.zm-editor-ruler-x{position:relative;display:inline-block;height:15px;min-width: 1400px;width:100%;overflow: hidden;white-space:nowrap}
.zm-editor-ruler-x>span{background-color:#333;width:1px; float: left;}
.zm-editor-ruler-x>span>span{position:absolute;top:-3px;left:0px;font-size: 9px;  -webkit-transform: scale(0.75);  -o-transform: scale(1);}
.zm-editor-ruler-x-span{height:5px;margin:10px 4px 0 0;}
.zm-editor-ruler-x-span2{ height:15px;margin:0 4px 0 0 ;position:relative;}
.zm-editor-ruler-x-firstSpan2{width:1px; height:15px;margin:0 4px 0 0 ;position:relative;}
.zm-editor-ruler-x-endSpan2{height:15px;position:relative;}

/*头部*/
.zm-header{background-color: @baseColorWhite;color:#000;}
.zm-header-top{height:50px;box-shadow: 0 0 8px #666}
.zm-header-top>div>div{float: left;height:51px;}
/*头部右侧*/
.zm-header-right{position:absolute;right:0;top:0;background: @baseColorWhite}
.zm-website-setting2{font-size: 13px;position:relative;}
.zm-website-setting2 img{height:20px;}
.zm-website-setting2>div{float: left;text-align: center;padding:0 10px;cursor:pointer}
.zm-website-setting2>div:hover{background-color:#ddd}
.zm-publish{background-color:@baseColor;  color: @baseColorWhite;cursor: pointer;position:relative;padding:15px 20px 0 20px;}
.zm-publish:hover{background: @baseColorDeep1}
/*站点选择*/
.zm-website-choiceBox{margin-right:5px;}
.zm-website-choice-title{float:left;padding-top:15px;}
.zm-website-choice-siteBox{position:relative;float:left;}
.zm-website-choice-siteBox:hover .zm-website-choice-siteBox-ul{display: block}
.zm-website-choice-nowSite{padding:15px 20px 0 20px;height:51px;background:@baseColorLight1;cursor:pointer;}
.zm-website-choice-nowSite-val{display:inline-block;width:200px;white-space: nowrap;text-overflow:ellipsis; overflow:hidden;}
.zm-website-choice-nowSite:hover{background:@baseColor;color:@baseColorWhite}
.zm-website-choice-nowSite-icon{float: right;margin-top:2px;}
.zm-website-choice-siteBox-ul{display:none;position:absolute;top:99%;left:0;background: @baseColorWhite;box-shadow: 0 0 5px #ccc;width:100%;z-index: 91}
.zm-website-choice-siteBox-ul li{padding:8px 20px;cursor:pointer;white-space: nowrap;text-overflow:ellipsis; overflow:hidden;}
.zm-website-choice-siteBox-ul li:hover{background: @baseColorLight1}

